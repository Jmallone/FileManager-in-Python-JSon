import json

f = open('disco.json')

data = json.load(f)

for i in data['blocks']['block_list']:
    print(i)
    print(data['blocks']['block_list'][str(i)])

data['blocks']['block_list'].update({'15': 'juliano.txt'})
data['blocks']['block_list'].pop('15')

with open('data.txt', 'w') as outfile:
    json.dump(data, outfile)

f.close()
